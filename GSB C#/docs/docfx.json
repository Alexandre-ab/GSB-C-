{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "../GSB C#.csproj"
          ],
          "cwd": "."
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "net8.0-windows"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "index.md",
          "articles/**.md",
          "toc.yml"
        ]
      },
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ],
        "dest": "api"
      }
    ],
    "resource": [
      {
        "files": [
          "styles/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "GSB WinForms",
      "_disableContribution": true,
      "_enableSearch": true
    },
    "template": [
      "default"
    ]
  }
}

